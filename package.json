{
  "name": "deepagentsjs",
  "version": "1.0.0",
  "description": "TypeScript implementation of Deep Agents - a library for building controllable AI agents with LangGraph",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "yarn clean && tsc",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "prepublishOnly": "yarn clean && yarn build",
    "prepack": "yarn build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langchain-ai/deepagentsjs.git"
  },
  "keywords": [
    "ai",
    "agents",
    "langgraph",
    "langchain",
    "typescript",
    "llm"
  ],
  "author": "LangChain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/langchain-ai/deepagentsjs/issues"
  },
  "homepage": "https://github.com/langchain-ai/deepagentsjs#readme",
  "dependencies": {
    "@langchain/anthropic": "^0.3.0",
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.53",
    "dotenv": "^17.2.1",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@tsconfig/recommended": "^1.0.8",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^15.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5"
  },
  "resolutions": {
    "@langchain/langgraph": ">=0.2.53",
    "@langchain/core": ">=0.3.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "packageManager": "yarn@1.22.22"
}
